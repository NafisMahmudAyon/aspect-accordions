{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "aspect-accordions/accordion",
  "version": "0.1.0",
  "title": "Accordion",
  "category": "aspect-accordions",
  "icon": "welcome-learn-more",
  "usesContext": [
    "postId"
  ],
  "attributes": {
    "globalOptions": {
      "type": "object",
      "default": {
        "activeItems": [],
        "multiple": false,
        "iconEnabled": true,
        "iconPosition": "right",
        "activeIcon": "chevron-down-icon",
        "activeIconType": "outline",
        "inactiveIcon": "chevron-up-icon",
        "inactiveIconType": "outline",
        "activeIconClassName": "size-6",
        "inactiveIconClassName": "size-6",
        "labelClassName": "",
        "activeLabelClassName": "",
        "headerClassName": "",
        "activeHeaderClassName": "",
        "contentClassName": "",
        "accordionClassName": ""
      }
    }
  },
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
  "viewScript": "file:./frontend.js"
}